<Page x:Class="Flarial.Launcher.Pages.SettingsBackupPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Flarial.Launcher.Pages"
      xmlns:styles="clr-namespace:Flarial.Launcher.Styles"
      mc:Ignorable="d" Loaded="SettingsBackupPage_OnLoaded"
      Title="SettingsBackupPage" Height="470" Width="555">
    <Page.Clip>
        <RectangleGeometry Rect="0,0,555,470"/>
    </Page.Clip>
    <ScrollViewer Margin="20" Background="#201A1B">
        <VirtualizingStackPanel Name="BackupStackPanel"/>
    </ScrollViewer>
</Page>
